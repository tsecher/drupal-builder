{
    "name": "lycanthrop/drupal-builder",
    "description": "Outil de cr√©ation de projet drupal",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "thomas.secher",
            "email": "thomas.secher@consertotech.pro"
        }
    ],
    "bin": [
        "drupal-builder"
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "tsecher/bim-runner": "~1",
        "symfony/dotenv": "^5.0"
    },
    "repositories": {
        "bim-runner": {
            "type": "vcs",
            "url": "https://github.com/tsecher/bim-runner.git"
        }
    },
    "require-dev": {
        "symfony/var-dumper": "^3"
    }
}
